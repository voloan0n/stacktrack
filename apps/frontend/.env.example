# Copy to `apps/frontend/.env.local` for local development.

# Where this Next.js app is served (used by server-side fetches back into the app).
NEXT_PUBLIC_APP_DOMAIN=localhost
NEXT_PUBLIC_APP_PROTOCOL=http

# Where the Next.js API proxy should reach the backend.
API_URL=http://localhost:4000

# Optional: internal base URL used by server actions (defaults to http://127.0.0.1:3000).
SERVER_INTERNAL_URL=http://127.0.0.1:3000
